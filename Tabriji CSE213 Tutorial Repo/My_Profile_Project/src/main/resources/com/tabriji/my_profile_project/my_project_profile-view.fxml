<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Line?>
<?import javafx.scene.text.Font?>
<AnchorPane fx:id="parentAnchorPane" prefHeight="650.0" prefWidth="850.0" style="-fx-background-color: #d7ccc8;"
            xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.tabriji.my_profile_project.MyProjectProfileController">
    <children>
        <Label layoutX="300.0" layoutY="28.0" text="Welcome to your Profile Manager">
            <font>
                <Font size="18.0"/>
            </font>
        </Label>
        <Line endX="100.0" layoutX="425.0" layoutY="58.0" startX="-100.0"/>
        <TextField fx:id="nameTextField" layoutX="66.0" layoutY="129.0" prefHeight="46.0" prefWidth="201.0"
                   promptText="Click to enter your name here"/>
        <TextArea layoutX="58.0" layoutY="379.0" prefHeight="200.0" prefWidth="735.0"/>
        <Button layoutX="398.0" layoutY="325.0" mnemonicParsing="false" text="Button"/>
        <RadioButton layoutX="66.0" layoutY="228.0" mnemonicParsing="false" text="Male"/>
        <RadioButton layoutX="146.0" layoutY="228.0" mnemonicParsing="false" text="Female"/>
        <Label layoutX="66.0" layoutY="194.0" text="Select your gender">
            <font>
                <Font size="16.0"/>
            </font>
        </Label>
        <DatePicker fx:id="dobDatePicker" layoutX="64.0" layoutY="279.0" promptText="Select Date of Birth"/>
        <ComboBox layoutX="534.0" layoutY="136.0" prefHeight="33.0" prefWidth="201.0" promptText="Select Occupation"/>
        <CheckBox layoutX="534.0" layoutY="252.0" mnemonicParsing="false" text="CheckBox"/>
        <CheckBox layoutX="655.0" layoutY="252.0" mnemonicParsing="false" text="CheckBox"/>
    </children>
</AnchorPane>
